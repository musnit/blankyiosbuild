<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "blanky"
    version     = "0.0.3" >
    
    <!-- versionCode is optional and Android only -->

    <name>Little George's Blanky</name>

    <description>
        An interactive, animated storybook. 
    </description>

    <author href="" email="musnit@gmail.com">
        Aidan Musnitzky 
    </author>

    <preference name="phonegap-version" value="3.7.0" />

    <gap:platform name="ios" />

    <preference name="orientation" value="landscape" />
    <preference name="fullscreen" value="true" />
    <preference name="deployment-target" value="7.0" />
    <feature name="http://api.phonegap.com/1.0/file" />
    <feature name="http://api.phonegap.com/1.0/media" />
</widget>
<gap:plugin name="com.telerik.plugins.wkwebview" version="0.2.5" />
<gap:plugin name="org.apache.cordova.file" version="1.3.1" />
<gap:plugin name="org.apache.cordova.media" version="0.2.13" />
